{
	"prettier.enable": true,
	"[javascript]": {
		"editor.codeActionsOnSave": {
			"source.fixAll": true
		},
		"editor.defaultFormatter": "dbaeumer.vscode-eslint"
	},
	"[typescript]": {
		"editor.codeActionsOnSave": {
			"source.fixAll": true
		},
		"editor.defaultFormatter": "dbaeumer.vscode-eslint"
	},
	"[javascriptreact]": {
		"editor.codeActionsOnSave": {
			"source.fixAll": true
		},
		"editor.defaultFormatter": "dbaeumer.vscode-eslint"
	},
	"[typescriptreact]": {
		"editor.codeActionsOnSave": {
			"source.fixAll": true
		},
		"editor.defaultFormatter": "dbaeumer.vscode-eslint"
	},
	"[json]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"[yaml]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	}
}
